{
    "coc.preferences.extensionUpdateCheck": "never",
    "suggest.autoTrigger": "none",
    "java.home": "/opt/openjdk-bin-17",
    "java.configuration.runtimes": [
        {
            "name": "JavaSE-1.8",
            "path": "/opt/openjdk-bin-8"
        },
        {
            "name": "JavaSE-11",
            "path": "/opt/openjdk-bin-11"
        },
        {
            "name": "JavaSE-17",
            "path": "/opt/openjdk-bin-17",
            "default": true
        }
    ],
    "java.jdt.ls.vmargs": "-javaagent:/home/slg/opt/lombok.jar",
    "java.debug.settings.showStaticVariables": true,
    "java.format.settings.url": "/home/slg/.config/coc/extensions/node_modules/coc-java/formatters/eclipse-formatter.xml",
    "java.completion.importOrder": [
        "java",
        "javax",
        "org",
        "com",
        "io"
    ],
    "java.completion.overwrite": "false",
    "java.completion.guessMethodArguments": "true",
    "java.sources.organizeImports.starThreshold": 6,
    "java.sources.organizeImports.staticStarThreshold": 6,
    "explorer.keyMappings.global": {
        "<S-Left>": ["wait", "collapse"],
        "<S-Right>": ["wait", "expandable?", "expand", "open"]
    }
}
