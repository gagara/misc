{
    "workspace.rootPatterns": [
        ".git",
        ".hg",
        ".vim",
        ".projections.json"
    ],
    "coc.preferences.extensionUpdateCheck": "never",
    "suggest.autoTrigger": "none",
    "diagnostic.warningSign": ">>",
    "java.jdt.ls.java.home": "/usr/lib/jvm/java-17-openjdk",
    "java.configuration.runtimes": [
        {
            "name": "JavaSE-1.8",
            "path": "/usr/lib/jvm/java-8-openjdk"
        },
        {
            "name": "JavaSE-11",
            "path": "/usr/lib/jvm/java-11-openjdk"
        },
        {
            "name": "JavaSE-17",
            "path": "/usr/lib/jvm/java-17-openjdk",
            "default": true
        }
    ],
    "java.jdt.ls.lombokSupport.enabled": false,
    "java.jdt.ls.vmargs": "-javaagent:/home/vgagara/opt/lombok.jar",
    "java.import.gradle.annotationProcessing.enabled": false,
    "java.compile.nullAnalysis.mode": "automatic",
    "java.debug.settings.showStaticVariables": true,
    "java.inlayHints.parameterNames.enabled": "none",
    "java.codeGeneration.useBlocks": true,
    "java.format.settings.url": "/home/vgagara/projects/Talend/etc/eclipse-formatter.xml",
    "java.sources.organizeImports.starThreshold": 6,
    "java.sources.organizeImports.staticStarThreshold": 6,
    "java.completion.guessMethodArguments": true,
    "java.completion.importOrder": [
        "java",
        "javax",
        "org",
        "com",
        "io"
    ],
    "java.completion.favoriteStaticMembers": [
        "org.junit.Assert.*",
        "org.junit.Assume.*",
        "org.junit.jupiter.api.Assertions.*",
        "org.junit.jupiter.api.Assumptions.*",
        "org.junit.jupiter.api.DynamicContainer.*",
        "org.junit.jupiter.api.DynamicTest.*",
        "org.mockito.Mockito.*",
        "org.mockito.ArgumentMatchers.*",
        "org.mockito.Answers.*",
        "org.hamcrest.MatcherAssert.*",
        "org.hamcrest.CoreMatchers.*",
        "org.apache.commons.collections.CollectionUtils",
        "org.apache.commons.lang3.ArrayUtils"
    ],
    "explorer.keyMappings.global": {
        "<S-Left>": [
            "wait",
            "collapse"
        ],
        "<S-Right>": [
            "wait",
            "expandable?",
            "expand",
            "open"
        ]
    },
    "pyright.inlayHints.functionReturnTypes": false,
    "pyright.inlayHints.variableTypes": false,
    "clangd.path": "~/.config/coc/extensions/coc-clangd-data/install/15.0.1/clangd_15.0.1/bin/clangd"
}
