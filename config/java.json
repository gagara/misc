{
	"configurations": {
		"Java Attach": {
            "adapter": {
				"extends": "vscode-java",
                "port": "${JAVA_DAP_PORT:$JAVA_DAP_PORT}",
				"sync_timeout": 90000,
				"async_timeout": 90000
			},
			"default": true,
			"configuration": {
				"request": "attach",
				"projectName": "${ProjectName}",
				"host": "127.0.0.1",
				"port": "${DebugPort:5005}"
			},
			"breakpoints": {
				"exception": {
					"caught": "N",
					"uncaught": "N"
				}
			}
		}
	}
}
