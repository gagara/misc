{
    "configurations": {
        "Java Attach": {
            "adapter": {
                "extends": "vscode-java",
                "sync_timeout": 90000,
                "async_timeout": 90000
            },
            "default": true,
            "configuration": {
                "request": "attach",
                "projectName": "${ProjectName}",
                "host": "127.0.0.1",
                "port": "${DebugPort:5005}"
            },
            "breakpoints": {
                "exception": {
                    "caught": "N",
                    "uncaught": "N"
                }
            }
        },
        "Java Run/Attach": {
            "adapter": {
                "extends": "vscode-java",
                "launch": {
                    "remote": {
                        "runCommand": [
                            "*${DebuggeeCommand}"
                        ]
                    },
                    "delay": "${DebuggeeStartDelay:5}"
                }
            },
            "remote-request": "launch",
            "remote-cmdLine": [],
            "configuration": {
                "request": "attach",
                "projectName": "${ProjectName}",
                "host": "127.0.0.1",
                "port": "${DebugPort:5005}"
            },
            "breakpoints": {
                "exception": {
                    "caught": "N",
                    "uncaught": "N"
                }
            }
        }
    }
}
